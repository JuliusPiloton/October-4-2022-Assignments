import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    Scanner in = new Scanner(System.in );
	    
	    System.out.print("Enter first number: ");
	    int x = in.nextInt(); 
	    System.out.print("Enter first number: ");
	    int y = in.nextInt();
	    
	    if(x>y)
	    System.out.print(x +" is larger");
	    else
	    System.out.print(y +" is larger");
	}
}
